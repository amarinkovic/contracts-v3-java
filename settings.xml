<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 https://maven.apache.org/xsd/settings-1.0.0.xsd">

    <!--
     This file is needed for CI to be able to pull in web3j-maven-plugin dependency,
     published from our github repo as a github package. Github packages
     require valid Personal Access Token.
    -->
    <servers>
        <server>
            <id>github</id>
            <username>${env.GITHUB_USER}</username>
            <password>${env.GIT_TOKEN}</password>
        </server>
    </servers>

</settings>
